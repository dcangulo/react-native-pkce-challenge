{
  "name": "react-native-pkce-challenge",
  "version": "5.2.1",
  "description": "Proof Key for Code Exchange (PKCE) challenge generator for React Native",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "files": [
    "src",
    "lib",
    "cpp",
    "android",
    "ios",
    "macos",
    "react-native-pkce-challenge.podspec",
    "!android/build",
    "!android/.cxx",
    "!android/.gradle",
    "!ios/build"
  ],
  "scripts": {
    "lint": "eslint . --ext .js,.ts",
    "prepare": "husky install; bob build",
    "test": "tap tests",
    "typescript": "tsc --noEmit"
  },
  "keywords": [
    "react-native",
    "react-native-web",
    "react-native-macos",
    "PKCE",
    "oauth2",
    "jsi",
    "c++"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/dcangulo/react-native-pkce-challenge.git"
  },
  "author": {
    "name": "David Angulo",
    "email": "hello@davidangulo.xyz",
    "url": "https://www.davidangulo.xyz"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dcangulo/react-native-pkce-challenge/issues"
  },
  "homepage": "https://github.com/dcangulo/react-native-pkce-challenge",
  "dependencies": {
    "crypto-js": "4.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.0",
    "@commitlint/config-conventional": "^18.4.2",
    "@types/crypto-js": "^4.2.0",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "eslint": "^8.8.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-import": "^2.22.0",
    "husky": "^8.0.1",
    "react-native-builder-bob": "^0.23.2",
    "tap": "^18.6.1",
    "typescript": "^5.0.2"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      "typescript"
    ]
  },
  "tap": {
    "allow-empty-coverage": true
  }
}
